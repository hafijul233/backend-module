{
    "name": "hafijul233/backend-module",
    "type": "laravel-module",
    "description": "Laravel AdminLTE 3 Backend Module using Bootstrap4",
    "support": {
        "issues": "https://github.com/hafijul233/backend-module/issues",
        "wiki": "https://github.com/hafijul233/backend-module/wiki",
        "source": "https://github.com/hafijul233/backend-module",
        "docs": "http://hafijulislam.me/projects/backend-module"
    },
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Hafijul Islam",
            "email": "hafijul233@gmail.com",
            "homepage": "http://www.hafijulislam.me",
            "role": "Maintainer"
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "kyslik/column-sortable": "^6.4",
        "owen-it/laravel-auditing": "^12.0",
        "almasaeed2010/adminlte": "~3.0",
        "joshbrw/laravel-module-installer": "^2.0",
        "nwidart/laravel-modules": "^8.2",
        "yoeunes/notify": "^1.0",
        "laravelcollective/html": "^6.2",
        "spatie/laravel-menu": "^3.7",
        "spatie/laravel-permission": "^5.1",
        "diglactic/laravel-breadcrumbs": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Backend\\Providers\\BackendServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\Backend\\": "/"
        },
        "exclude-from-classmap": [
            "/Tests/",
            "/test/",
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Backend\\Tests\\": "Tests/"
        }
    }
}
